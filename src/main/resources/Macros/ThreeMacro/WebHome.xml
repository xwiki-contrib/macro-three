## Include the three.js library
#if ($xcontext.action != 'edit')
  #set ($discard = $xwiki.jsx.use('Macros.ThreeMacro.Code.ThreeUtils'))
  #set ($discard = $xwiki.ssx.use('Macros.ThreeMacro.Code.ThreeUtils'))
