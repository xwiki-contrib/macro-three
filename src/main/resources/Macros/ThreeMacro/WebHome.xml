#set ($width = $wikimacro.parameters.width)
#set ($height = $wikimacro.parameters.height)
## Add "px" if the width or height are plain numbers
#if ($numbertool.toNumber($width).toString().equals($width))
  #set ($width = "${width}px")
#end
#if ($numbertool.toNumber($height).toString().equals($height))
  #set ($height = "${height}px")
#end

#set ($canvasId = $util.generateRandomString(5))

(% id="threeCanvas${canvasId}" class="three-canvas" style="width: ${width}; height: ${height}" %)(((
$!wikimacro.content
)))
{{/velocity}}</code>
