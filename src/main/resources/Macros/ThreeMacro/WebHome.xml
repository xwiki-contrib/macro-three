    <property>
      <code>{{velocity}}
## Include the three.js utils
#if ($xcontext.action != 'edit')

  #set ($discard = $xwiki.jsx.use('Macros.ThreeMacro.Code.ThreeUtils'))
  #set ($discard = $xwiki.ssx.use('Macros.ThreeMacro.Code.ThreeUtils'))

  #set ($width = $wikimacro.parameters.width)
  #set ($height = $wikimacro.parameters.height)
  ## Add "px" if the width or height are plain numbers
  #if ($numbertool.toNumber($width).toString().equals($width))
    #set ($width = "${width}px")
  #end
  #if ($numbertool.toNumber($height).toString().equals($height))
    #set ($height = "${height}px")
  #end

  ## Check if the file is specified to be an attachment
  #if ($wikimacro.parameters.attachment)
    #if ($wikimacro.parameters.document)
      #set ($attachmentDoc = $xwiki.getDocument($wikimacro.parameters.document))
      #set ($file = $attachmentDoc.getAttachmentURL($wikimacro.parameters.file))
    #else
      #set ($file = $doc.getAttachmentURL($wikimacro.parameters.file))
    #end
  #else
    #set ($file = $wikimacro.parameters.file)
  #end

  #set ($canvasId = $util.generateRandomString(5))

  (%
    id="threeCanvas${canvasId}"
    class="macro-three-container"
    style="width: ${width}; height: ${height}"
    data-file="${file}"
    data-type="$!wikimacro.parameters.type"
    data-zoom-out="$!wikimacro.parameters.defaultZoomOut"
    data-background="$!wikimacro.parameters.background"
    data-disable-controls="$!wikimacro.parameters.disableControls"
    data-auto-rotate="$!wikimacro.parameters.autoRotate"
    data-auto-rotate-speed="$!wikimacro.parameters.autoRotateSpeed"
  %)((($!wikimacro.content)))

#end
{{/velocity}}</code>
